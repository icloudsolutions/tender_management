<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="Definitions_ICS_Tender" targetNamespace="http://bpmn.io/schema/bpmn" exporter="bpmn-js (ICS Tender Management)" exporterVersion="18.0.2">

  <bpmn:collaboration id="Collaboration_TenderManagement">
    <bpmn:participant id="Participant_Company" name="Company (Internal Process)" processRef="Process_TenderManagement" />
    <bpmn:participant id="Participant_Customer" name="Customer / Government Entity" />
    <bpmn:participant id="Participant_Vendors" name="Vendors / Suppliers" />
    <bpmn:messageFlow id="MessageFlow_RFQ" name="RFQ" sourceRef="Task_SendRFQ" targetRef="Participant_Vendors" />
    <bpmn:messageFlow id="MessageFlow_Quotes" name="Quotations" sourceRef="Participant_Vendors" targetRef="Task_CollectVendorOffers" />
    <bpmn:messageFlow id="MessageFlow_Submit" name="Tender Submission" sourceRef="Task_SubmitTender" targetRef="Participant_Customer" />
    <bpmn:messageFlow id="MessageFlow_Decision" name="Decision" sourceRef="Participant_Customer" targetRef="Gateway_TenderDecision" />
  </bpmn:collaboration>

  <bpmn:process id="Process_TenderManagement" name="ICS Tender Management Process" isExecutable="true">

    <!-- START EVENT -->
    <bpmn:startEvent id="StartEvent_OpportunityIdentified" name="Opportunity&#10;Identified">
      <bpmn:outgoing>Flow_ToRegisterTender</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- PRE-AWARD PHASE -->
    <bpmn:userTask id="Task_RegisterTender" name="Register&#10;Tender">
      <bpmn:incoming>Flow_ToRegisterTender</bpmn:incoming>
      <bpmn:outgoing>Flow_ToTechnicalStudy</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_TechnicalStudy" name="Technical&#10;Study">
      <bpmn:incoming>Flow_ToTechnicalStudy</bpmn:incoming>
      <bpmn:outgoing>Flow_ToFinancialStudy</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_FinancialStudy" name="Financial&#10;Study">
      <bpmn:incoming>Flow_ToFinancialStudy</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSendRFQ</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_SendRFQ" name="Send RFQ&#10;to Vendors">
      <bpmn:incoming>Flow_ToSendRFQ</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCollectOffers</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:userTask id="Task_CollectVendorOffers" name="Collect&#10;Vendor Offers">
      <bpmn:incoming>Flow_ToCollectOffers</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCompareVendors</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_CompareVendors" name="Compare&#10;Vendors">
      <bpmn:incoming>Flow_ToCompareVendors</bpmn:incoming>
      <bpmn:outgoing>Flow_ToSelectVendors</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_SelectVendors" name="Select Best&#10;Vendors">
      <bpmn:incoming>Flow_ToSelectVendors</bpmn:incoming>
      <bpmn:outgoing>Flow_ToPrepareQuotation</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_PrepareQuotation" name="Prepare&#10;Quotation">
      <bpmn:incoming>Flow_ToPrepareQuotation</bpmn:incoming>
      <bpmn:outgoing>Flow_ToApproveQuotation</bpmn:outgoing>
    </bpmn:userTask>

    <!-- QUOTATION APPROVAL GATEWAY -->
    <bpmn:exclusiveGateway id="Gateway_QuotationApproved" name="Approved?">
      <bpmn:incoming>Flow_ToApproveQuotation</bpmn:incoming>
      <bpmn:incoming>Flow_QuotationRevised</bpmn:incoming>
      <bpmn:outgoing>Flow_QuotationApproved</bpmn:outgoing>
      <bpmn:outgoing>Flow_QuotationRejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:userTask id="Task_ReviseQuotation" name="Revise&#10;Quotation">
      <bpmn:incoming>Flow_QuotationRejected</bpmn:incoming>
      <bpmn:outgoing>Flow_QuotationRevised</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_SubmitTender" name="Submit&#10;Tender">
      <bpmn:incoming>Flow_QuotationApproved</bpmn:incoming>
      <bpmn:outgoing>Flow_ToEvaluation</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:userTask id="Task_UnderEvaluation" name="Under&#10;Evaluation">
      <bpmn:incoming>Flow_ToEvaluation</bpmn:incoming>
      <bpmn:outgoing>Flow_ToAwaitDecision</bpmn:outgoing>
    </bpmn:userTask>

    <!-- TENDER DECISION GATEWAY -->
    <bpmn:exclusiveGateway id="Gateway_TenderDecision" name="Decision?">
      <bpmn:incoming>Flow_ToAwaitDecision</bpmn:incoming>
      <bpmn:outgoing>Flow_TenderWon</bpmn:outgoing>
      <bpmn:outgoing>Flow_TenderLost</bpmn:outgoing>
      <bpmn:outgoing>Flow_TenderCancelled</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- WON PATH -->
    <bpmn:userTask id="Task_ReceiveAwardLetter" name="Receive&#10;Award Letter">
      <bpmn:incoming>Flow_TenderWon</bpmn:incoming>
      <bpmn:outgoing>Flow_ToProjectTypeCheck</bpmn:outgoing>
    </bpmn:userTask>

    <!-- PROJECT TYPE GATEWAY -->
    <bpmn:exclusiveGateway id="Gateway_ProjectType" name="Project&#10;Type?">
      <bpmn:incoming>Flow_ToProjectTypeCheck</bpmn:incoming>
      <bpmn:outgoing>Flow_SupplyProject</bpmn:outgoing>
      <bpmn:outgoing>Flow_OMProject</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- SUPPLY PROJECT SUBPROCESS -->
    <bpmn:subProcess id="SubProcess_SupplyProject" name="Supply Project (6 Phases)">
      <bpmn:incoming>Flow_SupplyProject</bpmn:incoming>
      <bpmn:outgoing>Flow_Supply_ToComplete</bpmn:outgoing>

      <bpmn:startEvent id="StartEvent_Supply" name="Start">
        <bpmn:outgoing>Flow_Supply_ToPhase1</bpmn:outgoing>
      </bpmn:startEvent>

      <bpmn:userTask id="Task_Supply_Phase1" name="Phase 1:&#10;Project Receipt">
        <bpmn:incoming>Flow_Supply_ToPhase1</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToPhase2</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_Supply_Phase2" name="Phase 2:&#10;Contracting">
        <bpmn:incoming>Flow_Supply_ToPhase2</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToPhase3</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_Supply_Phase3" name="Phase 3:&#10;Execution">
        <bpmn:incoming>Flow_Supply_ToPhase3</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToPhase4</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_Supply_Phase4" name="Phase 4:&#10;Prelim Handover">
        <bpmn:incoming>Flow_Supply_ToPhase4</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToPhase5</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_Supply_Phase5" name="Phase 5:&#10;Final Handover">
        <bpmn:incoming>Flow_Supply_ToPhase5</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToPhase6</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_Supply_Phase6" name="Phase 6:&#10;Invoicing">
        <bpmn:incoming>Flow_Supply_ToPhase6</bpmn:incoming>
        <bpmn:outgoing>Flow_Supply_ToEnd</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:endEvent id="EndEvent_Supply" name="Complete">
        <bpmn:incoming>Flow_Supply_ToEnd</bpmn:incoming>
      </bpmn:endEvent>

      <bpmn:sequenceFlow id="Flow_Supply_ToPhase1" sourceRef="StartEvent_Supply" targetRef="Task_Supply_Phase1" />
      <bpmn:sequenceFlow id="Flow_Supply_ToPhase2" sourceRef="Task_Supply_Phase1" targetRef="Task_Supply_Phase2" />
      <bpmn:sequenceFlow id="Flow_Supply_ToPhase3" sourceRef="Task_Supply_Phase2" targetRef="Task_Supply_Phase3" />
      <bpmn:sequenceFlow id="Flow_Supply_ToPhase4" sourceRef="Task_Supply_Phase3" targetRef="Task_Supply_Phase4" />
      <bpmn:sequenceFlow id="Flow_Supply_ToPhase5" sourceRef="Task_Supply_Phase4" targetRef="Task_Supply_Phase5" />
      <bpmn:sequenceFlow id="Flow_Supply_ToPhase6" sourceRef="Task_Supply_Phase5" targetRef="Task_Supply_Phase6" />
      <bpmn:sequenceFlow id="Flow_Supply_ToEnd" sourceRef="Task_Supply_Phase6" targetRef="EndEvent_Supply" />
    </bpmn:subProcess>

    <!-- O&M SERVICE SUBPROCESS -->
    <bpmn:subProcess id="SubProcess_OMService" name="O&amp;M Service (6 Phases + Loop)">
      <bpmn:incoming>Flow_OMProject</bpmn:incoming>
      <bpmn:outgoing>Flow_OM_ToComplete</bpmn:outgoing>

      <bpmn:startEvent id="StartEvent_OM" name="Start">
        <bpmn:outgoing>Flow_OM_ToPhase1</bpmn:outgoing>
      </bpmn:startEvent>

      <bpmn:userTask id="Task_OM_Phase1" name="Phase 1:&#10;Kickoff">
        <bpmn:incoming>Flow_OM_ToPhase1</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ToPhase2</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_OM_Phase2" name="Phase 2:&#10;Planning">
        <bpmn:incoming>Flow_OM_ToPhase2</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ToPhase3</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_OM_Phase3" name="Phase 3:&#10;Execution">
        <bpmn:incoming>Flow_OM_ToPhase3</bpmn:incoming>
        <bpmn:incoming>Flow_OM_Continue</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ToPhase4</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_OM_Phase4" name="Phase 4:&#10;Monitoring">
        <bpmn:incoming>Flow_OM_ToPhase4</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ToPhase5</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:userTask id="Task_OM_Phase5" name="Phase 5:&#10;Invoicing">
        <bpmn:incoming>Flow_OM_ToPhase5</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ContractEnd</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:exclusiveGateway id="Gateway_OM_ContractEnd" name="Contract&#10;End?">
        <bpmn:incoming>Flow_OM_ContractEnd</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_Continue</bpmn:outgoing>
        <bpmn:outgoing>Flow_OM_ToPhase6</bpmn:outgoing>
      </bpmn:exclusiveGateway>

      <bpmn:userTask id="Task_OM_Phase6" name="Phase 6:&#10;Closure">
        <bpmn:incoming>Flow_OM_ToPhase6</bpmn:incoming>
        <bpmn:outgoing>Flow_OM_ToEnd</bpmn:outgoing>
      </bpmn:userTask>

      <bpmn:endEvent id="EndEvent_OM" name="Complete">
        <bpmn:incoming>Flow_OM_ToEnd</bpmn:incoming>
      </bpmn:endEvent>

      <bpmn:sequenceFlow id="Flow_OM_ToPhase1" sourceRef="StartEvent_OM" targetRef="Task_OM_Phase1" />
      <bpmn:sequenceFlow id="Flow_OM_ToPhase2" sourceRef="Task_OM_Phase1" targetRef="Task_OM_Phase2" />
      <bpmn:sequenceFlow id="Flow_OM_ToPhase3" sourceRef="Task_OM_Phase2" targetRef="Task_OM_Phase3" />
      <bpmn:sequenceFlow id="Flow_OM_ToPhase4" sourceRef="Task_OM_Phase3" targetRef="Task_OM_Phase4" />
      <bpmn:sequenceFlow id="Flow_OM_ToPhase5" sourceRef="Task_OM_Phase4" targetRef="Task_OM_Phase5" />
      <bpmn:sequenceFlow id="Flow_OM_ContractEnd" sourceRef="Task_OM_Phase5" targetRef="Gateway_OM_ContractEnd" />
      <bpmn:sequenceFlow id="Flow_OM_Continue" name="Continue" sourceRef="Gateway_OM_ContractEnd" targetRef="Task_OM_Phase3" />
      <bpmn:sequenceFlow id="Flow_OM_ToPhase6" name="End" sourceRef="Gateway_OM_ContractEnd" targetRef="Task_OM_Phase6" />
      <bpmn:sequenceFlow id="Flow_OM_ToEnd" sourceRef="Task_OM_Phase6" targetRef="EndEvent_OM" />
    </bpmn:subProcess>

    <!-- END EVENTS -->
    <bpmn:endEvent id="EndEvent_SupplyComplete" name="Supply&#10;Complete">
      <bpmn:incoming>Flow_Supply_ToComplete</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_OMComplete" name="O&amp;M&#10;Complete">
      <bpmn:incoming>Flow_OM_ToComplete</bpmn:incoming>
    </bpmn:endEvent>

    <!-- LOST PATH -->
    <bpmn:userTask id="Task_DocumentLoss" name="Document&#10;Loss">
      <bpmn:incoming>Flow_TenderLost</bpmn:incoming>
      <bpmn:outgoing>Flow_ToLostEnd</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_TenderLost" name="Tender&#10;Lost">
      <bpmn:incoming>Flow_ToLostEnd</bpmn:incoming>
    </bpmn:endEvent>

    <!-- CANCELLED PATH -->
    <bpmn:userTask id="Task_DocumentCancellation" name="Document&#10;Cancellation">
      <bpmn:incoming>Flow_TenderCancelled</bpmn:incoming>
      <bpmn:outgoing>Flow_ToCancelledEnd</bpmn:outgoing>
    </bpmn:userTask>

    <bpmn:endEvent id="EndEvent_TenderCancelled" name="Tender&#10;Cancelled">
      <bpmn:incoming>Flow_ToCancelledEnd</bpmn:incoming>
    </bpmn:endEvent>

    <!-- SEQUENCE FLOWS -->
    <bpmn:sequenceFlow id="Flow_ToRegisterTender" sourceRef="StartEvent_OpportunityIdentified" targetRef="Task_RegisterTender" />
    <bpmn:sequenceFlow id="Flow_ToTechnicalStudy" sourceRef="Task_RegisterTender" targetRef="Task_TechnicalStudy" />
    <bpmn:sequenceFlow id="Flow_ToFinancialStudy" sourceRef="Task_TechnicalStudy" targetRef="Task_FinancialStudy" />
    <bpmn:sequenceFlow id="Flow_ToSendRFQ" sourceRef="Task_FinancialStudy" targetRef="Task_SendRFQ" />
    <bpmn:sequenceFlow id="Flow_ToCollectOffers" sourceRef="Task_SendRFQ" targetRef="Task_CollectVendorOffers" />
    <bpmn:sequenceFlow id="Flow_ToCompareVendors" sourceRef="Task_CollectVendorOffers" targetRef="Task_CompareVendors" />
    <bpmn:sequenceFlow id="Flow_ToSelectVendors" sourceRef="Task_CompareVendors" targetRef="Task_SelectVendors" />
    <bpmn:sequenceFlow id="Flow_ToPrepareQuotation" sourceRef="Task_SelectVendors" targetRef="Task_PrepareQuotation" />
    <bpmn:sequenceFlow id="Flow_ToApproveQuotation" sourceRef="Task_PrepareQuotation" targetRef="Gateway_QuotationApproved" />
    <bpmn:sequenceFlow id="Flow_QuotationApproved" name="Yes" sourceRef="Gateway_QuotationApproved" targetRef="Task_SubmitTender" />
    <bpmn:sequenceFlow id="Flow_QuotationRejected" name="No" sourceRef="Gateway_QuotationApproved" targetRef="Task_ReviseQuotation" />
    <bpmn:sequenceFlow id="Flow_QuotationRevised" sourceRef="Task_ReviseQuotation" targetRef="Gateway_QuotationApproved" />
    <bpmn:sequenceFlow id="Flow_ToEvaluation" sourceRef="Task_SubmitTender" targetRef="Task_UnderEvaluation" />
    <bpmn:sequenceFlow id="Flow_ToAwaitDecision" sourceRef="Task_UnderEvaluation" targetRef="Gateway_TenderDecision" />
    <bpmn:sequenceFlow id="Flow_TenderWon" name="Won" sourceRef="Gateway_TenderDecision" targetRef="Task_ReceiveAwardLetter" />
    <bpmn:sequenceFlow id="Flow_TenderLost" name="Lost" sourceRef="Gateway_TenderDecision" targetRef="Task_DocumentLoss" />
    <bpmn:sequenceFlow id="Flow_TenderCancelled" name="Cancelled" sourceRef="Gateway_TenderDecision" targetRef="Task_DocumentCancellation" />
    <bpmn:sequenceFlow id="Flow_ToProjectTypeCheck" sourceRef="Task_ReceiveAwardLetter" targetRef="Gateway_ProjectType" />
    <bpmn:sequenceFlow id="Flow_SupplyProject" name="Supply" sourceRef="Gateway_ProjectType" targetRef="SubProcess_SupplyProject" />
    <bpmn:sequenceFlow id="Flow_OMProject" name="O&amp;M" sourceRef="Gateway_ProjectType" targetRef="SubProcess_OMService" />
    <bpmn:sequenceFlow id="Flow_Supply_ToComplete" sourceRef="SubProcess_SupplyProject" targetRef="EndEvent_SupplyComplete" />
    <bpmn:sequenceFlow id="Flow_OM_ToComplete" sourceRef="SubProcess_OMService" targetRef="EndEvent_OMComplete" />
    <bpmn:sequenceFlow id="Flow_ToLostEnd" sourceRef="Task_DocumentLoss" targetRef="EndEvent_TenderLost" />
    <bpmn:sequenceFlow id="Flow_ToCancelledEnd" sourceRef="Task_DocumentCancellation" targetRef="EndEvent_TenderCancelled" />

  </bpmn:process>

