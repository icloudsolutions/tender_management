      <bpmndi:BPMNShape id="Gateway_TenderDecision_di" bpmnElement="Gateway_TenderDecision" isMarkerVisible="true">
        <dc:Bounds x="1935" y="225" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1934" y="201" width="51" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- WON PATH -->
      <bpmndi:BPMNShape id="Task_ReceiveAwardLetter_di" bpmnElement="Task_ReceiveAwardLetter">
        <dc:Bounds x="2040" y="210" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- PROJECT TYPE GATEWAY -->
      <bpmndi:BPMNShape id="Gateway_ProjectType_di" bpmnElement="Gateway_ProjectType" isMarkerVisible="true">
        <dc:Bounds x="2195" y="225" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2191" y="201" width="58" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- SUPPLY SUBPROCESS -->
      <bpmndi:BPMNShape id="SubProcess_SupplyProject_di" bpmnElement="SubProcess_SupplyProject" isExpanded="true">
        <dc:Bounds x="2300" y="120" width="1100" height="240" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="StartEvent_Supply_di" bpmnElement="StartEvent_Supply">
        <dc:Bounds x="2332" y="222" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2337" y="265" width="26" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Supply_Phase1_di" bpmnElement="Task_Supply_Phase1">
        <dc:Bounds x="2410" y="200" width="100" height="80" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Supply_Phase2_di" bpmnElement="Task_Supply_Phase2">
